{
  "name": "store-front",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "clear": "npx rimraf .next node_modules",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "dev": "npx rimraf .next && next -p 3000",
    "start": "npx rimraf .next && STAGE=prod next -p 3000",
    "stub": "stubby --watch --stubs 8882 --admin 8889 --tls 7443 --data __tests__/stubs/stubby.json",
    "start:stub": "concurrently 'npm run stub' 'npx rimraf .next && STAGE=stub next  -p 3000'",
    "test:integration": "jest --testPathPattern=.integration.test\\.ts --passWithNoTests",
    "test:unit": "jest --testPathPattern=.unit.test\\.ts --passWithNoTests"
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/parser": "^3.7.0",
    "@zeit/next-source-maps": "^0.0.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "next": "^9.2.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.2.2",
    "react-dom": "^16.12.0",
    "sass": "^1.26.10",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.10",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^26.0.1",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^1.19.1",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^3.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.7.4",
    "url-loader": "^4.1.0",
    "stubby": "^5.0.0"
  }
}
